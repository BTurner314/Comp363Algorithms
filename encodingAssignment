def encode(string):
	encodedString = ''
	
	for x in string:
		if x=='b':
			encodedString = encodedString + '00'
		elif x=='e':
			encodedString = encodedString + '0'
		elif x=='k':
			encodedString = encodedString + '000'
		elif x=='p':
			encodedString = encodedString + '1'
		elif x=='r':
			encodedString = encodedString + '01'
		
		encodedString = encodedString + ' '

	print(encodedString)

import time

startTime = time.time()
encode('beekeeper')
encodeTime = time.time()-startTime
print(f'{encodeTime:13f}')
